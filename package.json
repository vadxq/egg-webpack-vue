{
  "name": "egg-webpack-vue",
  "version": "1.0.0",
  "description": "egg webpack vue",
  "main": "index.js",
  "scripts": {
    "dev:server": "npx egg-bin dev",
    "dev:client": "cross-env NODE_ENV=development npx webpack --mode development --watch",
    "dev": "npx egg-bin dev | cross-env NODE_ENV=development npx webpack --mode development --watch",
    "build": "cross-env NODE_ENV=production npx webpack",
    "start": "npx egg-scripts start --port 8080 --daemon --title=egg-server-vadxq",
    "stop": "npx egg-scripts stop --title=egg-server-vadxq"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vadxq/egg-webpack-vue.git"
  },
  "keywords": [
    "egg",
    "webpack",
    "vue"
  ],
  "author": "vadxq",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/vadxq/egg-webpack-vue/issues"
  },
  "homepage": "https://github.com/vadxq/egg-webpack-vue#readme",
  "dependencies": {
    "egg": "^2.22.2",
    "egg-view-static": "^1.0.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "egg-bin": "^4.13.0",
    "egg-scripts": "^2.11.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "h": "^0.1.0",
    "html-webpack-plugin": "^3.2.0",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5"
  }
}
