{
  "name": "egg-webpack-vue_react",
  "version": "1.0.0",
  "description": "egg webpack vue",
  "main": "index.js",
  "scripts": {
    "dev:vue": "npx webpack --watch --env vuedevelopment",
    "dev:react": "npx webpack --env reactdevelopment --watch",
    "dev:svue": "npx egg-bin dev NODE_ENV=vueevelopment",
    "dev:sreact": "npx egg-bin dev NODE_ENV=reactevelopment",
    "dev:er": "npm run dev:sreact | npm run dev:react",
    "dev:ev": "npm run dev:svue | npm run dev:vue",
    "build:vue": "npx webpack --env vueproduction",
    "build:react": "npx webpack --env reactproduction",
    "start": "npx egg-scripts start --port 8080 --daemon --title=egg-server-vadxq",
    "stop": "npx egg-scripts stop --title=egg-server-vadxq"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vadxq/egg-webpack-vue.git"
  },
  "keywords": [
    "egg",
    "webpack",
    "vue"
  ],
  "author": "vadxq",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/vadxq/egg-webpack-vue/issues"
  },
  "homepage": "https://github.com/vadxq/egg-webpack-vue#readme",
  "dependencies": {
    "egg": "^2.22.2",
    "egg-view-static": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.6",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "b": "^2.0.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "egg-bin": "^4.13.0",
    "egg-scripts": "^2.11.0",
    "h": "^0.1.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.23.1",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-merge": "^4.2.1"
  }
}
